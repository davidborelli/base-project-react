{
  "roots": ["<rootDir>/src"],
  "modulePaths": ["<rootDir>/src"],
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{ts,tsx}",
    "!**/*.d.ts"
  ],
  "coverageDirectory": "coverage",
  "testEnvironment": "jsdom",
  "transform": {
    ".+\\.(ts|tsx)$": "ts-jest"
  },
  "moduleNameWrapper": {
    "@/(.*)": "<rootDir>/src/$1",
    "\\.scss$": "identity-obj-proxy"
  }
}
